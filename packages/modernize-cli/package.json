{
  "name": "modernize-cli",
  "version": "1.0.0",
  "description": "CLI to convert ES5 to modern JS using babel-preset-modernize.",
  "bin": {
    "modernize": "./dist/modernize.js"
  },
  "source": "./src/cli.js",
